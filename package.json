{
  "name": "hanzluo-wp",
  "version": "0.0.1",
  "description": "Source code for hanzluo.com",
  "scripts": {
    "start": "npm-run-all --parallel server-watch client-watch nodemon",
    "build": "npm-run-all --sequential server-build client-build",
    "prod": "cross-env NODE_ENV=production node src/server/index",
    "nodemon": "cross-env NODE_ENV=development nodemon src/server/index",
    "server-watch": "webpack --config webpack/config.server.js",
    "client-watch": "node webpack/dev.server.js",
    "client-build": "webpack --env.prod --config ./webpack/config.client.js",
    "server-build": "webpack --env.prod --config ./webpack/config.server.js",
    "lint:js": "eslint --fix src webpack constants.js",
    "clean": "rm dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "analysis": "webpack --env.prod --env.analysis --config ./webpack/config.client.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lhz516/hanzluo.git"
  },
  "keywords": [
    "Hanz"
  ],
  "author": "Hanz Luo",
  "license": "MIT",
  "eslintIgnore": [
    "dist"
  ],
  "dependencies": {
    "antd": "^3.20.3",
    "connected-react-router": "^6.5.2",
    "cookie-parser": "^1.4.4",
    "d3": "^5.9.7",
    "d3-cloud": "^1.2.5",
    "debug": "^4.1.1",
    "express": "^5.0.0-alpha.7",
    "history": "^4.9.0",
    "http-errors": "^1.7.3",
    "mongoose": "^5.6.5",
    "morgan": "^1.9.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-faux-dom": "^4.5.0",
    "react-helmet": "^5.2.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-simple-i18n": "^1.1.1",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "assets-webpack-plugin": "^3.9.10",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "ignore-loader": "^0.1.2",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  }
}
